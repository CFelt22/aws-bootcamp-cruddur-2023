#! /usr/bin/bash
set -e # stop if it fails at any point

CYAN='\033[1;36m'
NO_COLOR='\033[0m'
LABEL="db-setup"
printf "${CYAN}==== ${LABEL}${NO_COLOR}\n"

ABS_PATH=$(readlink -f "$0")
#echo $ABS_PATH
DB_PATH=$(dirname $ABS_PATH)
#echo $DB_PATH
DDB_PATH=$(dirname $ABS_PATH)
#echo $DDB_PATH

python "$DB_PATH/update_cognito_user_ids"
source "$DDB_PATH/seed"